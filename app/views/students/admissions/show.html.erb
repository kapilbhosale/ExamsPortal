<% logo_props = get_logo(request.subdomain) %>
<%= image_tag logo_props[:logo], width: logo_props[:width], height: logo_props[:height] %>

<%= form_tag students_admissions_path, method: :post, class: 'form-horizontal' do %>
  <div class="form-group">
    <label class="control-label col-sm-2" for="name">Name:</label>
    <div class="col-sm-6">
      <input type="text" class="form-control" id="name" placeholder="Name" name="name" required>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <button type="submit" class="btn btn-success">Submit</button>
    </div>
  </div>
  <%= link_to 'Pay now', @url, class: 'nav-link' %>

  <p> test 0012</p>
<% end %>